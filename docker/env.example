# amd64 for Linux | arm64v8 | for M2, M1
PLATFORM=amd64
# amd64 for Linux | arm64 | for M2, M1
PLATFORM_TRAEFIK=arm64
#rocky-linux | ubuntu | alpine | debian
CONFIG_SYSTEM=ubuntu
#none | cakephp | laravel | symfony | livewire | filament | volt | multidb | apiplatform | wordpress
CONFIG_MONOLITHIC=none
#none | laravel 12 | symfony 7.2 | cakePHP 5.1 | livewire 3 | filament 3 | volt 1.7.0 | multidb laravel | multidb symfony | apiplatform laravel | apiplatform symfony | wordpress latest
CONFIG_MONOLITHIC_VERSION=latest
#none | vue | react | svelte
CONFIG_FRONTEND=react
#none | react 18.3.1
CONFIG_FRONTEND_VERSION=18.3.1
#none | laravel | express | nestjs | symfony
CONFIG_BACKEND=laravel
#none | laravel 12 | symfony 7.2 | cakePHP 5.1
CONFIG_BACKEND_VERSION=12
#mysql | postgresql | mongodb database plus backend or monolithic name
CONFIG_DATABASE=mysql
#apache | nginx 
CONFIG_WEB_SERVER=nginx
DOCKER_STACK_SSL=true
TIMEZONE=Europe/Madrid
PROJECT_NAME=multi-template
VERSION=1.0.0
DOMAIN=multi-template.localhost
PROJECT_PATH=/projects/multi-template
